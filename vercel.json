{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/todos/:userEmail",
        "dest": "/server.js"
      },
      {
        "src": "/todos",
        "dest": "/server.js"
      },
      {
        "src": "/todos/:id",
        "dest": "/server.js"
      },
      {
        "src": "/todos/:id",
        "dest": "/server.js"
      },
      {
        "src": "/signup",
        "dest": "/server.js"
      },
      {
        "src": "/login",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }